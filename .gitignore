web/sites/*/modules/custom/*/node_modules/
web/sites/*/modules/custom/*/tests/
web/sites/*/modules/custom/*/tmp/
web/sites/*/modules/custom/*/vendor/
# Ignore files generated by the build process
/web/css/*.map
/web/js/*.map
/web/libraries/
/web/sites/*/files/
/web/sites/*/private/
/web/sites/*/modules/contrib/
/web/sites/*/themes/contrib/
/web/core/
/web/vendor/
